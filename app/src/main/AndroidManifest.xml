        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:targetApi="m">


        <activity android:name="com.Result.ResultActivity">

        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.Quiztion.QuizActivity" />
        <activity
            android:name="com.Helper.SplashActivity"
            android:theme="@style/SplashTheme">

        </activity>
<service

    android:name="com.FireBase.MyFcmService">


</service>
    </application>

</manifest>